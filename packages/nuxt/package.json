{
  "name": "@vue-email/nuxt",
  "version": "0.8.0-beta.8",
  "description": "Nuxt package for vue-email",
  "author": "David Arenas <davejs136@gmail.com> (https://github.com/Dave136/)",
  "homepage": "https://vuemail.net/",
  "license": "MIT",
  "type": "module",
  "types": "dist/nuxt.d.ts",
  "exports": {
    ".": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs",
      "default": "./dist/nuxt.mjs"
    }
  },
  "files": [
    "dist/*",
    "client/dist/*"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "clear": "rimraf dist node_modules"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dave136/vue-email",
    "directory": "packages/nuxt"
  },
  "bugs": {
    "url": "https://github.com/Dave136/vue-email/issues"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.0",
    "@nuxt/schema": "^3.7.4",
    "@vue-email/compiler": "workspace:*",
    "@vue-email/core": "workspace:*",
    "@vue-email/types": "workspace:*",
    "@vue-email/utils": "workspace:*"
  }
}
